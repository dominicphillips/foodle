    <div class="accordion-heading">
      <a class="accordion-toggle pass" data-toggle="collapse" data-parent="#accordion2" href="#{{_id}}">

		{{name}}

      </a>
    </div>
    <div id="{{_id}}" class="accordion-body collapse in">
      <div class="accordion-inner">
		<div class="span3">
		  <div class="tile maps">
		    <div class="fui-location"></div>
		    <small>{{distance}}m</small>
		  </div>
		</div>

		<img width="100%" src="{{pic_url}}" alt="">

		<div class="accordion-footer">
			<div class="address pull-left">

				<span class="fui-location"></span>
				{{address.street}},
				{{address.zip}}
				{{address.city}}



			</div>

			<a class="btn btn-danger btn-large pull-right" href="/restaurants/{{_id}}" target="">
			    <i class="icon-food"></i>
			</a>

		</div>






      </div>
    </div>

